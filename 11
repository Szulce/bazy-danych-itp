SELECT p.IMIE,d.NAZWISKO,k.PENSJA
FROM PRACOWNICY p, PRACOWNICY d, PRACOWNICY k;

SELECT p.NAZWA,s.NAZWISKO,s.PENSJA
FROM PROJEKTY p,PRACOWNICY s
WHERE p.SZEF_PROJEKTU=s.ID;

SELECT d.NAZWA,COUNT(p.ID_DZIALU) AS licz
FROM DZIALY d LEFT JOIN PRACOWNICY p
                  ON p.ID_DZIALU=d.ID_DZIALU
GROUP BY (d.ID_DZIALU,d.NAZWA);
                         
SELECT p.NAZWISKO,p.PENSJA,r.NAZWA
FROM DZIALY d,PRACOWNICY p LEFT OUTER JOIN  PROJEKTY r
                      ON p.PROJEKT = r.P_ID
WHERE p.ID_DZIALU=d.ID_DZIALU AND d.NAZWA='Programisci';

//5nie umiem
SELECT p.IMIE,p.NAZWISKO,c.NAZWISKO AS SZEF
FROM DZIALY d,PRACOWNICY p RIGHT OUTER JOIN  PROJEKTY r
                           ON p.PROJEKT = r.P_ID
                           RIGHT OUTER JOIN PRACOWNICY c
                           ON r.SZEF_PROJEKTU = c.ID
WHERE p.ID_DZIALU=d.ID_DZIALU AND d.NAZWA='Programisci';

