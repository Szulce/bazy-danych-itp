//ZLONCZENIE ZEWNETRZNE - BO SA OBSERWACJE KTORE NIE WYSTEPUJA W OBU TABELACH
SELECT p.IMIE,p.NAZWISKO,d.NAZWA
FROM PRACOWNICY p ,DZIALY d
WHERE p.ID_DZIALU =d.ID_DZIALU OR p.ID_DZIALU IS NULL;
//pracownicy bez dzialow
SELECT p.IMIE,p.NAZWISKO,d.NAZWA
FROM PRACOWNICY p LEFT JOIN DZIALY d
                    USING (ID_DZIALU);
//dzialy bez pracownikow
SELECT p.IMIE,p.NAZWISKO,d.NAZWA
FROM PRACOWNICY p RIGHT JOIN DZIALY d
                    USING (ID_DZIALU);
//ZLACZENIE WEWNETRZNE
SELECT p.IMIE,p.NAZWISKO,d.NAZWA
FROM PRACOWNICY p ,DZIALY d
WHERE p.ID_DZIALU =d.ID_DZIALU;

SELECT p.IMIE,p.NAZWISKO,d.NAZWA
FROM PRACOWNICY p JOIN DZIALY d
                    ON p.ID_DZIALU = d.ID_DZIALU;
                  
SELECT p.IMIE,p.NAZWISKO,d.NAZWA
FROM PRACOWNICY p JOIN DZIALY d
                    USING p.ID_DZIALU = d.ID_DZIALU;                  
